{

    "layout": {
        "title": true,
        "customJS": [ "jquery.peity.js" ],
        "widgets": [
            { "row": 1, "col": 1, "width": 2, "height": 2, "widget": "IterationStatus", "job": "IterationStatusJob", "config": "iteration-status" },
            { "row": 1, "col": 1, "width": 3, "height": 2, "widget": "PivotalMilestones", "job": "PivotalMilestonesJob", "config": "pivotal-milestones" },
            { "row": 2, "col": 1, "width": 2, "height": 1, "widget": "CodeCoverage", "job": "CodeCoverageJob", "config": "teamcity-codecoverage" },
            { "row": 3, "col": 1, "width": 2, "height": 1, "widget": "PointStats", "job": "PointStatsJob", "config": "PointPre" },
            { "row": 4, "col": 1, "width": 2, "height": 1, "widget": "PointCurrStats", "job": "PointCurrStatsJob", "config": "PointCurr" },
            { "row": 3, "col": 5, "width": 1, "height": 1, "widget": "VelocityCurr", "job": "VelocityCurrJob", "config": "Velocity" }

        ]
    },
    "config": {
        "teamcity-codecoverage": {
            "interval": 600000,
            "teamcityServer": "https://teamcity.doubleline.us",
            "buildConfiguration": "EdFiAlliance_WebApp2aUnitTesting",
            "widgetTitle": "Unit Test Code Coverage"
        },
        "pivotal-milestones": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 987234,
            "widgetTitle": "PivotalTracker Milestones"
        },
        "iteration-status": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 987234,
            "widgetTitle": "Iteration Status"
        },
        "PointPre": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 987234,
            "widgetTitle": "Point stats in lastest 3 iterations"
        },
        "PointCurr": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 987234,
            "widgetTitle": "Point stats in current iteration"
        },
        "Velocity": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 987234,
            "widgetTitle": "Current Velocity"
        }
    }
}
