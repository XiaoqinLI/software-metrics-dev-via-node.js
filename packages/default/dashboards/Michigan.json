{

    "layout": {
        "title": true,
        "customJS": [ "jquery.peity.js" ],
        "widgets": [
            { "row": 1, "col": 1, "width": 2, "height": 1, "widget": "IterationStatus", "job": "IterationStatusJob", "config": "iteration-status" },
            { "row": 1, "col": 1, "width": 3, "height": 2, "widget": "PivotalMilestones", "job": "PivotalMilestonesJob", "config": "pivotal-milestones" },
            { "row": 2, "col": 1, "width": 2, "height": 1, "widget": "NoCodeCoverage", "job": "NoCodeCoverageJob", "config": "no-codecoverage" },
            { "row": 3, "col": 1, "width": 2, "height": 1, "widget": "PointStats", "job": "PointStatsJob", "config": "PointPre" },
            { "row": 3, "col": 3, "width": 2, "height": 1, "widget": "StoryStats", "job": "StoryStatsJob", "config": "StoryPre" },
            { "row": 4, "col": 3, "width": 2, "height": 1, "widget": "StoryCurrStats", "job": "StoryCurrStatsJob", "config": "StoryCurr" },
            { "row": 4, "col": 1, "width": 2, "height": 1, "widget": "PointCurrStats", "job": "PointCurrStatsJob", "config": "PointCurr" },
            { "row": 3, "col": 5, "width": 1, "height": 1, "widget": "VelocityCurr", "job": "VelocityCurrJob", "config": "Velocity" }

        ]
    },
    "config": {
        "no-codecoverage": {
            "interval": 600000,
            "widgetTitle": "Unit Test Code Coverage"
        },
        "pivotal-milestones": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "PivotalTracker Milestones"
        },
        "iteration-status": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Iteration Status"
        },
        "PointPre": {
            "interval": 60000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Point stats in lastest 3 iterations"
        },
        "StoryPre": {
            "interval": 60000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Story stats in lastest 3 iterations"
        },
        "StoryCurr": {
            "interval": 60000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Story stats in current iteration"
        },
        "PointCurr": {
            "interval": 60000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Point stats in current iteration"
        },
        "Velocity": {
            "interval": 600000,
            "pivotalTrackerServer": "https://www.pivotaltracker.com",
            "projectId": 1064986,
            "widgetTitle": "Current Velocity"
        }
    }
}
